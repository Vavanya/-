CREATE TABLE "student" (
  "id_student" int PRIMARY KEY,
  "first_name" varchar NOT NULL,
  "second_name" varchar NOT NULL,
  "patronymic" varchar,
  "arrival_time" date NOT NULL,
  "form_training" varchar NOT NULL,
  "group_number" int,
  "name_group" varchar,
  "title_specialty" varchar,
  "assessments" varchar
);

CREATE TABLE "assessments_card_student" (
  "name_training" varchar,
  "name_training_id" varchar,
  "exams_or_offset" varchar,
  "skipping_classes" int,
  "average_mark" int,
  "themes" id
);

CREATE TABLE "a_card_student" (
  "class_theme" varchar,
  "hours_theme" int,
  "number_of_classes" int,
  "test_ball" int
);

CREATE TABLE "inf_training" (
  "form_training" varchar,
  "id_training" SERIAL PRIMARY KEY
);

CREATE TABLE "assessments" (
  "id_assessments" int PRIMARY KEY,
  "full_name_student" varchar,
  "years" int,
  "semestr" int
);

CREATE TABLE "s" (
  "id_student" int,
  "id_training" int,
  PRIMARY KEY ("id_student", "id_training")
);

CREATE TABLE "academic_plan_specialty" (
  "name_training" varchar,
  "name_training_id" int PRIMARY KEY
);

CREATE TABLE "academi_plan_specialty_plan" (
  "name_training" varchar,
  "hours" int,
  "number_of_classes" int
);

CREATE TABLE "academic_plan_students" (
  "name_training" varchar,
  "name_training_id" int PRIMARY KEY
);

CREATE TABLE "a_card_student_1" (
  "class_theme_id" int PRIMARY KEY,
  "class_theme" varchar
);

ALTER TABLE "s" ADD FOREIGN KEY ("id_training") REFERENCES "inf_training" ("id_training");

ALTER TABLE "student" ADD FOREIGN KEY ("id_student") REFERENCES "s" ("id_student");

ALTER TABLE "student" ADD FOREIGN KEY ("form_training") REFERENCES "inf_training" ("form_training");

ALTER TABLE "assessments" ADD FOREIGN KEY ("id_assessments") REFERENCES "student" ("id_student");

ALTER TABLE "academic_plan_students" ADD FOREIGN KEY ("name_training_id") REFERENCES "assessments" ("id_assessments");

ALTER TABLE "assessments_card_student" ADD FOREIGN KEY ("name_training_id") REFERENCES "academic_plan_students" ("name_training_id");

ALTER TABLE "a_card_student_1" ADD FOREIGN KEY ("class_theme_id") REFERENCES "assessments_card_student" ("themes");

ALTER TABLE "a_card_student" ADD FOREIGN KEY ("class_theme") REFERENCES "a_card_student_1" ("class_theme_id");

ALTER TABLE "academic_plan_specialty" ADD FOREIGN KEY ("name_training_id") REFERENCES "student" ("title_specialty");

ALTER TABLE "academi_plan_specialty_plan" ADD FOREIGN KEY ("name_training") REFERENCES "academic_plan_specialty" ("name_training_id");

